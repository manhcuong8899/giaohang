                                                    <input type="hidden" name="_csrf" value="{{csrfToken}}">
                                                    <div class="single-input-item mb-3">
                                                        <label for="display-name" class="required mb-1">Họ và Tên</label>
                                                        <input type="text" id="full_name" name="full_name" placeholder="Họ và Tên" value="{{data.amember.full_name}}" />
                                                    </div>
                                                    <div class="single-input-item mb-3">
                                                        <label for="email" class="required mb-1">Địa chỉ Email</label>
                                                        <input type="text" id="email" name="email" placeholder="Địa chỉ Email" value="{{data.amember.email}}" />
                                                    </div>
                                                    <div class="single-input-item mb-3">
                                                        <label for="email" class="required mb-1">Địa chỉ</label>
                                                        <input type="text" id="address" name="address" placeholder="Địa chỉ" value="{{data.amember.address}}" />
                                                    </div>
